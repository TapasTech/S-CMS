{
  "name": "S-CMS",
  "version": "0.1.1",
  "private": true,
  "description": "The super content management system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/TapasTech/S-CMS.git"
  },
  "engines": {
    "node": ">=4.2.4"
  },
  "scripts": {
    "test": "jest",
    "start": "node ./node_modules/.bin/tapas-build",
    "build": "node ./node_modules/.bin/tapas-build --production",
    "check": "eslint src"
  },
  "main": "./src/index.js",
  "jest": {
    "scriptPreprocessor": "<rootDir>/scripts/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-router",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/lodash"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "tapas": {
    "index": "./src/index.html",
    "entry": "./src/index.js",
    "output": "./build",
    "port": "8080",
    "aliasRoot": "#",
    "proxy": {
      "/api": "http://developer.dtcj.com:23000"
    }
  },
  "dependencies": {
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-mixin": "^3.0.3",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-multi": "^0.1.10",
    "redux-router": "^1.0.0-beta7",
    "redux-thunk": "^1.0.3",
    "tapas-ui": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "jest-cli": "^0.8.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "tapas-build": "^0.2.11"
  }
}
